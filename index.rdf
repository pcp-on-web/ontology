	<http://creativecommons.org/ns#attributionName> "AKSW", "HAB", "HTWK" ;
	dc:title "Professorale Karrieremuster der Frühen Neuzeit - Version 0.1 (pcp-on-web)"@de,
  	"Early Modern Professorial Career Patterns - Version 0.1 (pcp-on-web)"@en;
	dc:license "http://creativecommons.org/licenses/by/4.0/" ;
	dc:modified "2021-02-03";
	<http://purl.org/vocab/vann/changes> <https://github.com/pcp-on-web/ontology/commits/master/index.rdf> ;
	<http://purl.org/vocab/vann/preferredNamespacePrefix> "pcp" ;
	<http://purl.org/vocab/vann/preferredNamespaceUri> "http://pcp-on-web.de/ontology#" ;
	<http://usefulinc.com/ns/doap#homepage> <https://pcp-on-web.de/project/> ;
  <http://usefulinc.com/ns/doap#maintainer> <https://pcp-on-web.de/team/JenniferBlanke>, <https://pcp-on-web.de/team/ThomasRiechert> ;
	dc:description "The PCP-on-Web Model defines the research model for the research projekt Early Modern Professorial Career Patterns Methodological research on online databases of academic history.";
	rdfs:label "Professorale Karrieremuster der Frühen Neuzeit - Version 0.1 (pcp-on-web)"@de,
  	"Early Modern Professorial Career Patterns - Version 0.1 (pcp-on-web)"@en;
  	owl:versionInfo "Version 0.1 (inital) - 2021-02-03 - Jennifer Blanke";
  	dc:creator "Jennifer Blanke, Edgard Marx, Thomas Riechert" ;
  	<http://xmlns.com/foaf/0.1/maker> <http://pcp-on-web.de/team/JenniferBlanke> , <http://pcp-on-web.de/team/EdgardMarx> , <http://pcp-on-web.de/team/ThomasRiechert>.

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gDay
xsd:gDay rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gMonth
xsd:gMonth rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gMonthDay
xsd:gMonthDay rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYearMonth
xsd:gYearMonth rdf:type rdfs:Datatype .		
		
		
#################################################################
#		Classes
#################################################################

:AcademicOffice rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Akademisches Amt"@de, "Academic office"@en ;
	rdfs:comment "Akademische Ämter wie Professuren sowie akademische Nebenämter, bspw. als Rektor, Prorektor, Kanzler, Dekan usw. ."@de , "Academic offices like professorships and further academic offices, e.g. rector, prorector, chancellor, dean etc.."@en .
		
:Article rdf:type owl:Class ;
  rdfs:subClassOf :Publication ;
  rdfs:label "Artikel"@de, "Article"@en ;
	rdfs:comment "Artikel in einem Journal, einer Zeitschrift, einer Zeitung oder einem Magazin."@de, "Articles published in journals, newspapers or magazines."@en .
		
:AsscociateProfessor rdf:type owl:Class ;
  rdfs:subClassOf :AcademicOffice ;
  rdfs:label "Außerordentlicher Professor"@de, "Associate Professor"@en ;
	rdfs:comment "Bezeichnet einen Professor, der eine außerordentliche Professur (aO) inne hat. Diese Professoren auf Anwartschaft lehrten ohne Lehrstuhl und warteten in der regel auf Vakanz."@de, "Associate professors who teach without holding a chair. Those professors usually taught in expectance of a vacant chair."@en .	
		
:Baccalaureus rdf:type owl:Class ;
  rdfs:subClassOf :Graduation ;
  rdfs:label "Baccalaureus"@de, "Baccalaureus"@en ;
	rdfs:comment "Bezeichnet den akademischen Grad eines Baccalaren/Baccalaureus. Kürzel: Bacc."@de , "Describes the academic degree of a bachelor/baccalaureus.  Abbreviation: Bacc."@en .
		
:Birth rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Birth> ;
  rdfs:label "Geburt"@de, "Birth"@en ;
	rdfs:comment "Geburtsdatum oder Zeitraum einer Person."@de, "Birthdate or the period of birth of a person."@en .

:Candidacy rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Kandidatur"@de, "Candidacy"@en ;
	rdfs:comment "Bezeichnet die Kandidatur einer Person, die im Zuge der Besetzung einer Professur als Kandidat gehandelt wird. Diese Person kann ein Kandidat ohne Lehrstuhl sein, aber auch ein amtierender Professor, der für einen anderen Lehrstuhl gehandelt wird."@de , "Refers to a candidacy of a person which is a candidate for a university chair or professorship. This person can be a candidate without a chair, but also an incumbent professor who is being traded for another chair."@en .
		
:Chair rdf:type owl:Class ;
  rdfs:subClassOf :Faculty ;
  rdfs:label "Lehrstuhl"@de, "Chair"@en ;
	rdfs:comment "Bezeichnet den Lehrstuhl bzw. das Fachgebiet an einer Universitätsfakultät."@de, "Designates the chair or subject at a university faculty."@en .

:Collection rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Sammlung"@de, "Collection"@en ;
	rdfs:comment "Objekt-Sammlungen eines Professors, z.B. Privatbibliotheken, Naturalien- und Instrumentensammlungen etc."@de, "Object-collections of a professor, e.g. private libraries, naturalia or further scholarly devices."@en .
		
:Corporation rdf:type owl:Class ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Body> ;
  rdfs:subClassOf dc:Agent ;
  rdfs:label "Körperschaft"@de, "Corporation"@en ;
	rdfs:comment "Bezeichnet eine Körperschaft."@de, "Refers to a corporation or corporate body."@en .		

:Correspondence rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Korrespondenz"@de, "Correspondence"@en ;
	rdfs:comment "Verweist auf Korrespondenzpartner einer Person."@de, "Refers to correspondence partners of a person."@en .

:Curriculum rdf:type owl:Class ;
  rdfs:subClassOf :Document ;
  rdfs:label "Lektionskatalog"@de, "Curriculum"@en ;
	rdfs:comment "Lektions- oder Vorlesungskatalog einer Universität."@de, "Curriculum or lecture catalogue of a university."@en .
	
:Denomination rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#denomination> ;
  rdfs:label "Konfession"@de, "Denomination"@en ;
	rdfs:comment "Religionszugehörigkeit oder Glaubenbekenntnis einer Person."@de , "The religious denomination of a person."@en .
		
:Death rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Death> ;
  rdfs:label "Tod"@de, "Death"@en ;
	rdfs:comment "Sterbedatum oder den Zeitraum des Todes einer Person."@de, "Day of death or the period of death of a person."@en .				     

:Dissertation rdf:type owl:Class ;
  rdfs:subClassOf :Publication ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#QualificationDocument> ;
  rdfs:label "Dissertation"@de, "Dissertation"@en ;
	rdfs:comment "Bezeichnet das gedruckte Medium der Disputation."@de, "Refers to the printed media of a disputation."@en .
		
:Disputation rdf:type owl:Class ;
  rdfs:subClassOf :TeachingPractices;
  rdfs:label "Disputation"@de, "Disputation"@en ;
	rdfs:comment "Bezieht sich auf die Disputationspraxis, dem wissenschaftlichen Streitgespräch. Es diente dem Zweck der Übung oder der Graduierung und verwies als Inaugural Disputation pro loco auf die Erlangung einer Professur. Die Dissertation ist das gedruckte Medium der Dispuation."@de, "Refers to the practice of disputation, a scientific debate which had the purpose of practising curricula or to obtain academic titles. It also refers to inaugural disputations of newly vocated professors. The printed medium of a disputation is the dissertation."@en .
		
:Doctor rdf:type owl:Class ;
  rdfs:subClassOf :Graduation ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#GraduationDoctorate> ;
  rdfs:label "Doctor"@de, "Doctor"@en ;
	rdfs:comment "Bezeichnet den akademischen Grad eines Doktoren. Kürzel: Dr."@de , "Describes the academic degree of a doctor. Abbreviation: Dr."@en .

:Document rdf:type owl:Class ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Document> ;
  rdfs:subClassOf <http://purl.org/spar/fabio/Expression> ;
  rdfs:subClassOf <http://purl.org/dc/terms/BibliographicResource> ;
  rdfs:label "Dokument"@de, "Document"@en ;
	rdfs:comment "Beschreibt ein Dokument."@de, "General Document."@en .
		
:Education  rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Bildungsweg"@de, "Education"@en ;
	rdfs:comment "Beschreibt den Bildungsweg einer Person."@de , "Decribes the education of a person."@en .

:Enrollment rdf:type owl:Class ;
  rdfs:subClassOf :UniversityStudies ;
  rdfs:label "Immatrikulation"@de, "Enrollment"@en ;
	rdfs:comment "Bezieht sich auf die Einschreibung einer Person in das Universitätsverzeichnis, die (Universitäts-)Matrikel."@de, "Refers to the enrollment of a person into the universities' directory, the (university) 'Matrikel'."@en .

:Exercise rdf:type owl:Class ;
  rdfs:subClassOf :TeachingPractices;
  rdfs:label "Übung"@de, "Exercise"@en ;
	rdfs:comment "Bezieht sich auf die Praktik der Übung."@de, "Refers to the practice of exercise."@en .
		
:Faculty rdf:type owl:Class ;
  rdfs:subClassOf :University ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Faculty> ;
  rdfs:label "Fakultät"@de, "Faculty"@en ;
	rdfs:comment "Bezieht sich auf eine körperschaftlich organisierte und auf ein Fachgebiet begrenzte Lehr- und Verwaltungseinheit einer Volluniversität, im frühneuzeitlichen Kontext: Artistenfakultät (facultas artium), später Philosophische Fak., Theologie, Jurisprudenz, Medizin."@de, "Refers to corporated university divisions with specific subject areas, which are in an Early Modern context: facultas artium, later Philosophical fac., Theology, Jurisprudence, Medicine."@en .

:FacultyOfLaw rdf:type owl:Class ;
  rdfs:subClassOf :Faculty ;
  rdfs:label "Juristische Fakultät"@de, "Faculty of Law"@en ;
	rdfs:comment "Bezeichnet die Juristische Fakultät an einer Universität."@de, "Refers to the Faculty of Law at Universities."@en .
		
:FacultyOfMedicine rdf:type owl:Class ;
  rdfs:subClassOf :Faculty ;
  rdfs:label "Medizinische Fakultät"@de, "Faculty of Medicine"@en ;
	rdfs:comment "Bezeichnet die Medzinische Fakultät an einer Universität."@de, "Refers to the Faculty of Medicine at Universities."@en .
		
:FacultyOfPhilosophy rdf:type owl:Class ;
  rdfs:subClassOf :Faculty ;
  rdfs:label "Philosophische Fakultät"@de, "Faculty of Philosophy"@en ;
	rdfs:comment "Bezeichnet die Artesfakultät, die sich im Untersuchungszeitraum zu einer Philosophischen Fakultät entwickelte und an der propädeutisches Wissen für das weitergehende Studium an den übrigen höheren Fakultäten einer Volluniversität vermittelt wurde."@de, "Refers to the facultats artium which developed into a faculty of philosophy throughout the period of interest and at which propaedeutic knowledge was taught for further studies at one of the higher faculties of a full university."@en .
		
:FacultyOfTheology rdf:type owl:Class ;
  rdfs:subClassOf :Faculty ;
  rdfs:label "Theologische Fakultät"@de, "Faculty of Theology"@en ;
	rdfs:comment "Bezeichnet die Theologische Fakultät an einer Universität."@de, "Refers to the Faculty of Theology at Universities."@en .
		
:Family rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Family> ;
  rdfs:label "Familie"@de, "Family"@en ;
	rdfs:comment "Dieses Konzept beschreibt die soziale Herkunft und Familienverhältnisse einer Person."@de, "This concept describes the social origin and the family relationship of a person."@en .
		
:Female rdf:type owl:Class ;
  rdfs:subClassOf :Gender ;
  rdfs:label "Weiblich"@de, "Female"@en ;
	rdfs:comment "Verweist auf das Geschlecht Weiblich."@de, "Refers to the gender female."@en .
		
:Gender rdf:type owl:Class ;
  rdfs:subClassOf :Person ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#gender> ;
  rdfs:label "Geschlecht"@de, "Gender"@en ;
	rdfs:comment "Verweist auf das Geschlecht einer Person."@de, "Refers to the gender of a person."@en .
		
:Graduation rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Graduation> ;
  rdfs:label "Abschluss"@de, "Graduation"@en ;
	rdfs:comment "Bezeichnet einen Abschluss an einer Schule oder Universität."@de, "Refers to a graduation at a school or university."@en .
		
:Honour rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Honour> ;
  rdfs:label "Ehrung"@de , "Honour"@en ;
	rdfs:comment "Diese Konzept beschreibt eine Ehrung"@de, "The concept Person describes an honour"@en .
		
:Institution rdf:type owl:Class ;
  rdfs:subClassOf :Corporation ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Institution> ;
  rdfs:label "Institution"@de , "Institution"@en ;
	rdfs:comment "..."@de, "..."@en .
		
:Keyword rdf:type owl:Class ;
  rdfs:label "Schlagwort"@de, "Keyword"@en ;
	rdfs:comment "Zusätzliche Informationen, die in den Datenbanken unter Schlagworten und Kommentarfunktionen verborgen sind und ganz unterschiedlicher Natur sein können."@de, "Additional information given under keyword or commentary in the original databases and which can be varied in content."@en . 
		
:LearnedSociety rdf:type owl:Class ;
  rdfs:subClassOf :Societies ;
  rdfs:label "Gelehrte Gesellschaft"@de, "Learned Society"@en ;
	rdfs:comment "Bezeichnet eine Gelehrte Gesellschaft, die sich einer bestimmten Geistesströmung oder Disziplin widmete und aus Gelehrten und Gebildeten bestand sowie zumeist regional begrenzt agierte."@de, "Refers to a learned society which dealt with a certain discipline or ideas which gathered learned and educated people, mostly of regionaly importance."@en .
		
:Lecture rdf:type owl:Class ;
  rdfs:subClassOf :TeachingPractices ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Lecture> ;
  rdfs:label "Vorlesung/Lektion"@de, "Lecture"@en ;
	rdfs:comment "Bezieht sich auf Vorlesungen bzw. Lektionen (lectiones), die von einem Professor im Rahmen der universitären Lehre in Vorlesungsverzeichnissen angekündigt wurden."@de,
		"Refers to lectures announced by a professor in the curriculum of a university."@en .
		
:LectureDone rdf:type owl:Class ;
  rdfs:subClassOf :Lecture ;
  rdfs:label "abgehaltene Vorlesung/Lektion"@de, "Lecture done"@en ;
	rdfs:comment "Bezieht sich auf Vorlesungen bzw. Lektionen (lectiones) die von Professoren nachweislich durchgeführt wurden, z.B. nachgewiesen in Rechenschaftsberichten."@de,
		"Refers to lectures actually given by a professor and which can be proved, e.g. reports."@en .	
		
:Licentiatus rdf:type owl:Class ;
  rdfs:subClassOf :Graduation ;
  rdfs:label "Lizenziat"@de, "Licentiate"@en ;
	rdfs:comment "Bezeichnet den akademischen Grad eines Lizenziaten, eines Baccalaren mit Lehrbefugnis. Zugleich einen Doktor der evang. Theologie (bis 1947 an deutschen Hochschulen). Kürzel: lic."@de , "Describes the academic degree of a Licentiate, which means a bachelor/baccalaureus with the right to teach. Also the academic title of a doctor of Evangelic theology (valid until 1947 in Germany). Abbreviation: lic."@en .
		
:Location rdf:type owl:Class ;
  rdfs:subClassOf wgs84:SpatialThing;	
  rdfs:label "Ort"@de, "Location"@en ;
	rdfs:comment "Bezieht sich auf den geografischen Ort für Lebensabschnitte einer Person, Orte von Körperschaften, Medien (Verlagsort, Druckort), Ämtern, Landesherrschaft usw."@de, "Refers to the geographical location of life stages of persons, corporate bodies, media (printing, publishing), offices, territories and others."@en .

:Magister rdf:type owl:Class ;
  rdfs:subClassOf :Graduation ;
  rdfs:label "Magister"@de, "Magister"@en ;
	rdfs:comment "Bezeichnet den akademischen Grad eines Magister. Kürzel: Mag."@de , "Describes the academic degree of a Magister. Abbreviation: Mag."@en .
		
:Male rdf:type owl:Class ;
  rdfs:subClassOf :Gender ;
  rdfs:label "Männlich"@de, "Male"@en ;
	rdfs:comment "Verweist auf das Geschlecht Männlich."@de, "Refers to the gender male."@en .

:Membership rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Membership> ;
  rdfs:label "Mitgliedschaft"@de, "Membership"@en ;
	rdfs:comment "Verweist auf Mitgliedschaften einer Person in soziokulturellen und/oder politischen Gesellschaften, Sozietäten und weiterer Institutionen; auch Studentenverbindungen."@de, "Refers to the membership of a person at sociocultural and/or political societies or further institutions; also fraternities."@en .
		
:Office rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Office> ;
  rdfs:label "Amt"@de, "Office"@en ;
	rdfs:comment "Bezieht sich auf nicht akademische Ämter bzw. Nebenberufe von Professoren außerhalb der Universität, z.B. als Ärzte, Räte oder Anwälte."@de , "Refers to non-academic offices resp. side professions of professors outside of the university, e.g. as physicians at court, lawyers or counselers."@en .

:Pamphlet rdf:type owl:Class ;
  rdfs:subClassOf :Publication ;
  rdfs:label "Streitschrift"@de, "Pamphlet"@en ;
	rdfs:comment "Bezeichnet eine Streitschrift."@de, "Refers to a pamphlet."@en .
		
:Patron rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Patron"@de, "Patron"@en ;
	rdfs:comment "Bezieht sich auf den Patron einer Person; hier auf in den Quellen identifizierbaren Förderer eines Professors und dessen Karriere."@de ,
		"Refers to the patron of a person; here to the patron of a professor's career who can be identified by the sources."@en .	
		
:Person rdf:type owl:Class ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Person> ;
  rdfs:subClassOf dc:Agent ;
  rdf:label "Person"@de, "Person"@en ;
	rdfs:comment "Bezeichnet eine Person."@de, "Refers to a person."@en .
		
:Picture rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;		
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Picture> ;
  rdfs:label "Bild"@de, "Picture"@en ;
	rdfs:comment "Dieses Konzept beschreibt das Bild einer Person."@de, "This concept describes the picture of a person."@en .
		
:PrimaryEducation  rdf:type owl:Class ;
  rdfs:subClassOf :Education ;
  rdfs:label "Primäre Bildung"@de, "Primary education"@en ;
	rdfs:comment "Bezieht sich auf die diversen vormodernen Schulformen und pädagogischen Bildungsangebote im Vorfeld eines Universitätsbesuchs im Territorium des Heiligen Römischen Reichs Deutscher Nation, z.B. Elementarschulen, Lateinschulen, Gymnasien, Klosterschulen, Pfarrschulen etc."@de , "Refers to the diverse early modern educational and pedagogical formas and institutions of primary education within the Holy Roman Empire."@en .		

:Privatdozent rdf:type owl:Class ;
  rdfs:subClassOf :AcademicOffice ;
  rdfs:label "Privatdozent"@de, "Privatdozent"@en ;
	rdfs:comment "in progress: Bezeichnet Lehrkräfte ohne Professur, oft als PD bezeichnet. Magister oder Doktoren, die mit Lehrbefähigung an der Universität lehrten, aber kein Salär erhielten, sondern von den direkten Einnahmen ihrer Lehrpraxis lebten."@de, "Refers to teachers without professorships, often referred to as PD. Those were Magister or Doctors who did not receive a salary but got paid directly for their teaching practices."@en .
		
:PrivateEducation rdf:type owl:Class ;
  rdfs:subClassOf :Education ;
  rdfs:label "Privatunterricht"@de, "Private education"@en ;
	rdfs:comment "Privatunterricht im Familienverband oder durch einen Privatlehrer."@de,
		"Private education by family members or a different private teacher."@en .
		
:PrivateLecture rdf:type owl:Class ;
  rdfs:subClassOf :Lecture;
  rdfs:label "Private Vorlesung"@de, "Private Lecture"@en ;
	rdfs:comment "Bezieht sich auf private Vorlesungen (lectio privata, privatum, privatissimum), die ein Professor in einem kleinen Kreis von Studenten hält und die von den Studenten in der Regel an den Professor selbst bezahlt werden mussten."@de, "Refers to private lectures (lectio privata, privatum, privatissimum), given by a professor for a relatively small amount of students, which a 	student usually had to pay for directly to the Professor."@en .
		
:Professor rdf:type owl:Class ;
  rdfs:subClassOf :Person ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Professor> ;
  rdfs:label "Professor"@de, "Professor"@en ;
	rdfs:comment "Bezeichnet ordentliche Professoren an frühneuzeitlichen Universitäten im deutschsprachigen Raum des Heiligen Römischen Reichs, deren Karrierewege im Fokus des für die Ontologie maßgebenden Forschungsprojektes liegen."@de,
		"Refers to full or ordinary professors at early modern universities in the German-speaking area of the Holy Roman Empire whose career paths are the focus of the research project relevant to the given ontology."@en .

:Professorship rdf:type owl:Class ;
  rdfs:subClassOf :AcademicOffice ;
  rdfs:label "Professur"@de, "Professorship"@en ;
	rdfs:comment "Bezeichnet den Lehrstuhl eines ordentlichen Professors an einer Universitätsfakultät, der einem Fachgebiet zugeordnet ist."@de, "Designates the chair of a full or ordinary Professor at a university faculty."@en .
		
:ProgramsAndSpeeches rdf:type owl:Class ;
  rdfs:subClassOf :Publication ;
  rdfs:label "Programme und Reden"@de, "Programs and Speeches"@en ;
	rdfs:comment "Bezieht sich auf gedruckte Universitätsprogramme und Reden."@de, "Refers to printed university programs and speeches."@en .

:Publication rdf:type owl:Class ;
  rdfs:subClassOf :Document ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#Publication> ;
  rdfs:label "Publikation"@de, "Publication"@en ;
