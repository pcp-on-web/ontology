  rdfs:range :Person ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#familyChild> ;
  rdfs:label "hat Kind"@de, "has child"@en ;
	rdfs:comment "Bezieht sich auf das Kind, Pflegekind, Adoptivkind eines Professors."@de, "Refers to the cild, foster child, adoptive child of a professor."@en .

:hasContributor rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf dc:contributor;
  rdfs:range :Person, :Corporation ;
  rdfs:domain :Document ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#hasTutor>, <http://purl.org/pcp-on-web/ontology#author>, <http://purl.org/pcp-on-web/ontology#published>, <https://d-nb.info/standards/elementset/gnd#author> ;
  rdfs:label "hat Beteiligten"@de, "has contributor"@en ;
	rdfs:comment "Verweist auf eine Entität, die am Entstehungsprozess eines Dokuments beteiligt ist."@de, "Refers to the contributor in the developement process of a document."@en .	        
	      
:hasCreator rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf dc:creator, :hasContributor ;
  rdfs:domain :Document ;
  rdfs:range :Corporation, :Person ;
  rdfs:label "hat Urheber"@de, "has creator"@en ;
	rdfs:comment "Urheber eines Dokuments"@de, "Creator of a document."@en .	      

:hasDedicatee rdf:type owl:ObjectProperty ;
  rdfs:domain :Dissertation ;
  rdfs:range :Person ;
  rdfs:label "hat Widmungsempfänger"@de, "has dedicatee"@en ;
	rdfs:comment "Bezeichnet den Widmunsgempfänger einer Publikation."@de, "Refers to the dedicatee of a publication."@en .
	      
:hasEnrollment rdf:type owl:ObjectProperty ;
  rdfs:domain :Person ;
  rdfs:range :Enrollment ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#hasEnrollment> ;  
  rdfs:label "hat Matrikel"@de, "has enrollment"@en ;
	rdfs:comment "Verweist auf die Immatrikulation einer Person an einer Universität."@de, "Refers to the enrollment of a person at a university."@en .

:hasFaculty rdf:type owl:ObjectProperty ;
  rdfs:domain :AcademicDocuments, :AcademicOffice ;
  rdfs:range :Faculty ;
  rdfs:label "hat Fakultät"@de, "has faculty"@en ;
	rdfs:comment "Verknüpft Dokumente und Ämter mit einer Fakultät."@de, "Links documents and offices to a faculty."@en .	      

:hasKeyword rdf:type owl:ObjectProperty ;
  rdfs:domain :Document ;
  rdfs:range :Keyword ;
  rdfs:label "hat Schlagwort"@de, "has keyword"@en ;
	rdfs:comment "Verknüpft Dokumente mit Schlagwortangaben."@de, "Links documents to keyword specification."@en .  
	      
:hasOpponent rdf:type owl:ObjectProperty ;
  rdfs:domain :Dissertation ;
  rdfs:range :Person ;
  rdfs:label "hat Opponent"@de, "has opponent"@en ;
	rdfs:comment "Bezeichnet den oder die Personen, die während einer Disputation als Opponenten auftreten. Opponenten sind auf Titelblättern der gedruckten Disputationsschrift in der Regel nicht angegeben und konnten aus Lehrkräften sowie Studenten gleichermaßen bestehen."@de, "Refers to the persons who acted as opponents at a disputation. Opponents are rarely indicated in printed disputation papers aka dissertations and could consist of teachers as well as students."@en .	      

:hasOrigin rdf:type owl:ObjectProperty ;
  rdfs:domain :Person ;
  rdfs:range :Location ;
  rdfs:label "hat Herkunft"@de, "has origin"@en ;
	rdfs:comment "Verweist auf die geografische Herkunft von Personen."@de, "Refers to the geographical origin of persons."@en .	      	      

:hasParent rdf:type owl:ObjectProperty ;
  rdfs:domain :Family ;
  rdfs:range :Person ;
  rdfs:label "hat Elternteil"@de, "has parent"@en ;
	rdfs:comment "Verweist auf personen, die als Eltern auftreten."@de, "Refers to persons who appear as parents."@en .	      
	      
:hasPraeses rdf:type owl:ObjectProperty ;
  rdfs:domain :Dissertation ;
  rdfs:range :Person ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#praeses> ;
  rdfs:label "hat Praeses"@de, "has praeses"@en ;
	rdfs:comment "Bezeichnet die Person, die während einer Disputation als Praeses auftritt und die Disputation leitet."@de, "Refers to the person who is the praeses - the head - of a disputation."@en .	      

:hasPublisher rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf dc:publisher;
  rdfs:domain :Document ;
  rdfs:range :Corporation, :Person ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#published> ;
  rdfs:label "hat Herausgeber"@de, "has publisher"@en ;
	rdfs:comment "Nennt Herausgeber eines Dokuments."@de, "Gives the publisher of a document."@en .	      
	      
:hasRecommendation rdf:type owl:ObjectProperty ;
  rdfs:domain :Candidacy ;
  rdfs:range :Recommendation ;
  rdfs:label "hat Empfehlung"@de, "has recommendation"@en ;
	rdfs:comment "Gibt an, ob ein Kandidat ein Empfehlungsschreiben hat."@de, "Refers to any given recommendation of a candidate."@en .

:hasRespondent rdf:type owl:ObjectProperty ;
  rdfs:domain :Dissertation ;
  rdfs:range :Person ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#respondent> ;
  rdfs:label "hat Respondent"@de, "has respondent"@en ;
	rdfs:comment "Bezeichnet die Person des Respondenten, der sich der Disputation unterzieht."@de, "Refers to the person who is undergoing the act of disputation, called respondent."@en .   	      
	      
:hasSchool rdf:type owl:ObjectProperty ;
  rdfs:domain :PrimaryEducation ;
  rdfs:range :School ;
  rdfs:label "hat Schule"@de, "has school"@en ;
	rdfs:comment "Verweist auf die Institution an der primäre Bildung erworben wurde."@de, "Refers to the institution of primary education."@en .
	      
:hasSemester rdf:type owl:ObjectProperty ;
  rdfs:domain :UniversityStudies, :TeachingPractices ;
  rdfs:range :Semester ;
  rdfs:label "hat Semester"@de, "has Semester"@en ;
    rdfs:comment "Nennt Semester eines Studiums."@de,"Gives the semester of a university study."@en .

:hasStageOfLife rdf:type owl:ObjectProperty ;
  rdfs:domain :Lecturer ;
  rdfs:range :StageOfLife ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#hasPeriod> ;
  rdfs:label "hat Lebensabschnitt"@de, "has stage of life"@en ;
	rdfs:comment "Lebensabschnitt einer Lehrkraft."@de, "Stage of life of a lecturer."@en .

:hasSubject rdf:type owl:ObjectProperty ;
  rdfs:domain :Dissertation ;
  rdfs:range :Subject ;
  rdfs:label "hat Fach"@de, "has Subject"@en ;
    rdfs:comment "Ordnet der Dissertation ein Fachgebiet zu."@de,"Assigns a dissertation to a subject."@en .	      
	      
:hasWell-wisher rdf:type owl:ObjectProperty ;
  rdfs:domain :Dissertation ;
  rdfs:range :Person ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#wellWisher> ;
  rdfs:label "hat Gratulant"@de, "has well-wisher"@en ;
	rdfs:comment "Bezeichnet den Gratulanten einer Publikation, bspw. den Gratulanten eines Disputanten. Diese Gratulanten-Schriften befinden sich zumeist am Ende einer Dissertation."@de, "Refers to the well-wisher of a publication, e.g. the well-wisher to a disputant. These well-wishing writings are usually found at the end of a dissertation."@en .

:hasHusband rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf :hasParent ;
  rdfs:domain :Family ;
  rdfs:range :Person ;
  rdfs:label "hat Ehemann"@de, "has husband"@en ;
	rdfs:comment "Bezieht sich auf Personen, die als Ehemann auftreten."@de, "Refers to persons who appear as husbands."@en .	      
	      
:isDoing rdf:type owl:ObjectProperty ;
  rdfs:domain :Lecturer ;
  rdfs:range :TeachingPractices ;
  rdfs:label "führt durch"@de, "does"@en ;
	rdfs:comment "Bezieht sich auf Lehrpraktiken, die von einer Lehrkraft an einer Universität durchgeführt werden, z.B. Vorlesungen halten, Disputationen vorstehen oder Übungen geben."@de, "Refers to teaching practices which are done by a lecturer at a university, e.g. give lectures, preside over disputations or give excersizes."@en.

:isPartOfFaculty rdf:type owl:ObjectProperty ;
  rdfs:domain :ChairProfessorship ;
  rdfs:range :Faculty ;
  rdfs:label "ist Teil der Fakultät"@de, "is part of faculty"@en ;
	rdfs:comment "Bezieht sich auf Lehrstühle der einzelnen Fakultäten."@de, "Refers to chairs of the individual faculties."@en .     
	      
:isPartOfUniversity rdf:type owl:ObjectProperty ;
  rdfs:domain :Faculty ;
  rdfs:range :University ;
  rdfs:label "ist Teil der Universität"@de, "is part of university"@en ;
	rdfs:comment "Bezieht sich auf konstituierende Bestandteile einer Universität."@de, "Refers to the constituent parts of a university."@en .	      
	      
:isVocated rdf:type owl:ObjectProperty ;
  rdfs:domain :Candidacy ;
  rdfs:range :ChairProfessorship, :AssociateProfessor ;
  rdfs:label "wird ernannt"@de, "is vocated"@en ;
	rdfs:comment "Bezieht sich auf den von Universität und Landesherrschaft ausgewählten Kandidaten für eine Professur."@de, "Refers to the candidate for a professorship/or chair which had been negotiated by the university and the territorial authority."@en .

:lectureIsOfType rdf:type owl:ObjectProperty ;
  rdfs:domain :Lecture ;
  rdfs:range :LectureType ;
  rdfs:label "Vorlesung ist Typ"@de, "lecture is of type"@en ;
	rdfs:comment "Verweist auf den spezifischen Typ einer Vorlesung."@de, "Refers to the specific type of a lecture."@en .	      
	      
:locationCorporation rdf:type owl:ObjectProperty ;
  rdfs:domain :Corporation ;
  rdfs:range :Location ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#bodyPlace> ;
  rdfs:label "Ort einer Körperschaft"@de, "location of a corporation"@en ;
	rdfs:comment "Bezieht sich auf den geografischen Ort einer Körperschaft."@de,
		"Refers to the geographical location of a corporation."@en .

:locationDocument rdf:type owl:ObjectProperty ;
  rdfs:domain :Document ;
  rdfs:range :Location ;
  rdfs:label "Ort eines Dokument"@de, "location of a document"@en ;
	rdfs:comment "Bezieht sich auf den geografischen Ort eines Dokuments, z.B. Publikationsort."@de,
		"Refers to the geographical location of a document, e.g. location of a publication."@en .	   

:male rdf:type owl:ObjectProperty ;
  rdfs:domain :Person ;
  rdfs:range :Gender ;
  rdfs:label "männlich"@de, "male"@en ;
  	rdfs:comment "Verweist auf das biologische männliche Geschlecht oder die männliche Geschlechtsidentität einer Person."@de, "Refers to the male sex or male gender of a person."@en .	      
	      
:membershipSociety rdf:type owl:ObjectProperty ;
  rdfs:domain :Membership ;
  rdfs:range :Societies ;
  rdfs:label "Mitgliedschaft hat Institution"@de, "membership has institution"@en ;
	rdfs:comment "Weist die Mitgliedschaft einer Institution zu."@de, "Adds the membership to an institution."@en .
				     				     
:nominates rdf:type owl:ObjectProperty ;
  rdfs:domain :Faculty ;
  rdfs:range :Candidacy ;
  rdfs:label "nominiert"@de, "nominates"@en ;
	rdfs:comment "Bezieht sich auf Nominierungslisten einer Fakultät, die an die Verwaltung der Landesherrschaft erging und aus denen ein Kandidat durch die Landesherrschaft bestätigt wurde. Diese Listen liegen zumeist in zuständigen Archiven vor."@de,
	"Refers to nomination lists which were handed in  to the territorial administration by the faculties in  order to choose and to vocate a  candidate for a university chair. Those lists can be found in relevant archives."@en .			     

:owns rdf:type owl:ObjectProperty ;
  rdfs:domain :Lecturer ;
  rdfs:range :Collection ;
  rdfs:label "besitzt"@de, "owns"@en ;
	rdfs:comment "Verweist auf Sammlungen einer Lehrkraft, z.B. Privatbibliotheken, Münzen und anderweitige fachspezifische Sammlungen, die bspw. für eine Person im Berufungsverfahren auf eine Professur von Bedeutung gewesen sein könnten."@de, "Refers to a collection of a lecturer, like private libraries, coins and other subject specific collections, which could have been important for the vocation of a professorship."@en .      
	      
:relatedDocument rdf:type owl:ObjectProperty ;
  rdfs:domain :StageOfLife;
  rdfs:range :Document;
  rdfs:label "Relevante Dokumente von einem Lebensabschnitt"@de, "relevant documents of a stage of life"@en ;
	rdfs:comment "Ordnet einem Lebensabschnitt Dokumente zu, z.B. "@de, "Relevant documents of a stage of life."@en .	      
	      	      
:stageOfLifeLocation rdf:type owl:ObjectProperty ;
  rdfs:domain :StageOfLife ;
  rdfs:range :Location ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#periodPlace> ;
  rdfs:label "Ort Lebensabschnitt"@de, "location of stage of life"@en ;
	rdfs:comment "Fügt dem Lebensabschnitt einer Lehrkraft einen geografischen Ort zu."@de, "Adds a geographical location to a stage of life of a lecturer."@en .
	      
:student rdf:type owl:ObjectProperty ;
  rdfs:domain :Lecturer ;
  rdfs:range :Student ;
  rdfs:label "ist Student"@de, "is student"@en ;
	rdfs:comment "Weist eine Lehrkraft als Student aus."@de, "Refers to the lecturer as a student."@en .
	      
:hasWife rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf :hasParent ;
  rdfs:domain :Family ;
  rdfs:range :Person ;
  rdfs:label "hat Ehefrau"@de, "has wife"@en ;
	rdfs:comment "Bezieht sich auf die Person, die als Ehefrau auftritt."@de, "Refers to the person who appears as a wife."@en .

:tagged rdf:type owl:ObjectProperty ;
  rdfs:derivedFrom <http://purl.org/pcp-on-web/ontology#furtherInformation>, <http://purl.org/pcp-on-web/ontology#note>, <http://purl.org/pcp-on-web/ontology#keyword> ;
  rdfs:domain owl:Thing ;
  rdfs:range :Keyword ;
  rdfs:label "markiert"@de, "tagged"@en ;
	rdfs:comment "Gibt Informationen, die unspezifisch in Datenbanken unter Schlagworten, Kommentaren oder Notizen verborgen sind."@de, "Gives information that is unspecifically hidden in databases under keywords, comments or notes."@en .
	      
:vocates rdf:type owl:ObjectProperty ;
  rdfs:domain :Territory ;
  rdfs:range :Lecturer ;
  rdfs:label "beruft"@de, "vocates"@en ;
	rdfs:comment "Landesherrschaft beruft bzw. bestätigt und ernennt eine Lehrkraft."@de, "Territorial rulership vocates or confirms a lecturer."@en .

	
