#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gDay
xsd:gDay rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gMonth
xsd:gMonth rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gMonthDay
xsd:gMonthDay rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYearMonth
xsd:gYearMonth rdf:type rdfs:Datatype .		
		
:date rdf:type owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:domain :StageOfLife ;
  rdfs:range rdfs:Literal;
  rdfs:label "Date"@en, "Datum"@de ;
  	dc:descritption "Zeitangabe eines Lebensabschnitts."@de, "The date or timespan of a stage of life from a valid datatype
		 xsd:date ,
                 xsd:gDay ,
                 xsd:gMonth ,
                 xsd:gMonthDay ,
                 xsd:gYear ,
                 xsd:gYearMonth"@en .
		
###  http://purl.org/pcp-on-web/ontology#from
:from rdf:type owl:DatatypeProperty ;
      rdfs:domain :StageOfLife ;
      rdfs:range rdfs:Literal ;
      rdfs:comment "Das Startdatum eines Lebensabschnitts"@de ,
                   "The date of the beginning of a stage of life from a valid datatype xsd:date ,
                 xsd:gDay ,
                 xsd:gMonth ,
                 xsd:gMonthDay ,
                 xsd:gYear ,
                 xsd:gYearMonth"@en ;
      rdfs:label "From"@en ,
                 "Von"@de ;
      owl:versionInfo "0.1 initial creation date:  24.05.2007"^^xsd:string ,
                      "2.0 stable other types added"^^xsd:string .
		
###  http://purl.org/pcp-on-web/ontology#to
:to rdf:type owl:DatatypeProperty ;
    rdfs:domain :StageOfLife ;
    rdfs:range rdfs:Literal ;
    rdfs:comment "Das Enddatum eines Lebensabschnitts"@de ,
                 "The date of the ending of a stage of life from a valid datatype xsd:date ,
               xsd:gDay,
               xsd:gMonth,
               xsd:gMonthDay,
               xsd:gYear,
               xsd:gYearMonth"@en ;
    rdfs:label "Bis"@de ,
               "To"@en ;
    owl:versionInfo "0.1 initial creation date:  24.05.2007"^^xsd:string ,
                    "2.0 stable other types added"^^xsd:string .
		
##################################################################################		
	
		
:Person rdf:type owl:Class ;
  rdf:label "Person"@de, "Person"@en ;
	dc:description "Bezeichnet eine historische Person."@de, "Refers to a historical person."@en .
		
:Gender rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Geschlecht"@de, "Gender"@en ;
	dc:description "Verweist auf das Geschlecht einer Person."@de, "Refers to the gender of a person."@en .
		
:Male rdf:type owl:Class ;
  rdfs:subClassOf :Gender ;
  rdfs:label "Männlich"@de, "Male"@en ;
	dc:description "Verweist auf das Geschlecht Männlich."@de, "Refers to the gender male."@en .
		
:Female rdf:type owl:Class ;
  rdfs:subClassOf :Gender ;
  rdfs:label "Weiblich"@de, "Female"@en ;
	dc:description "Verweist auf das Geschlecht Weiblich."@de, "Refers to the gender female."@en .
		
:hasGender rdf:type owl:DatatypeProperty ;
  rdfs:domain :Person ;
  rdfs:range :Gender ;
  rdfs:label "hat Geschlecht"@de, "has gender"@en ;
	dc:description "Weist einer Person ein Geschlecht zu."@de, "Allocates a person to gender."@en .

:Professor rdf:type owl:Class ;
  rdfs:subClassOf :Person ;
  rdfs:label "Professor"@de, "Professor"@en ;
	dc:description "Bezeichnet eine Person die durch Berufung an einer frühneuzeitlichen Universität lehrt und dort akademische Ämter führt."@de,
		"Refers to a person who is vocated as a professor at an Early Modern university, who teaches and holds academic offices."@en .
		
:FullProfessor rdf:type owl:Class ;
  rdfs:subClassOf :Professor ;
  rdfs:label "Ordentlicher Professor"@de, "Full Professor"@en ;
	dc:description "Bezeichnet einen Professor, der eine ordentliche Professur inne hat."@de, "A professor who owns a full professorship."@en . 

:AsscociateProfessor rdf:type owl:Class ;
  rdfs:subClassOf :Professor ;
  rdfs:label "Außerordentlicher Professor"@de, "Associate Professor"@en ;
	dc:description "Bezeichnet einen Professor, der eine außerordentliche Professur (aO) inne hat."@de, "A professor who is an associate professor."@en .

###  http://purl.org/pcp-on-web/ontology#surname
:surname rdf:type owl:DatatypeProperty ;
  rdfs:domain :Person ;
  rdfs:range rdfs:Literal ;
  rdfs:label "Nachname"@de , "Surname"@en ;
  rdfs:comment "Nachname einer Person"@de , "The surname of a person"@en ;
		         owl:versionInfo "0.1 initial creation date: 24.05.2007"^^xsd:string ,
                         "2.0 stable"^^xsd:string ,
                         "version 0.2  10.03.2008"^^xsd:string .

###  http://purl.org/pcp-on-web/ontology#additionToSurname
:additionToSurname rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :Person ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Additions to the surname can be of, etc."@en ,
                                "Namenszusätze sind zum Beispiel von, zu, etc."@de ;
                   rdfs:label "Additions to Surename"@en ,
                              "Namenszusatz"@de ;
                   owl:versionInfo "0.2 changed Domain from Professor to Person 0.1 initial creation - 24.05.07"^^xsd:string ,
                                   "2.0 stable"^^xsd:string .


###  http://purl.org/pcp-on-web/ontology#alternativeWritingOfSurname
:alternativeWritingOfSurname rdf:type owl:DatatypeProperty ,
                                      owl:FunctionalProperty ;
                             rdfs:domain :Person ;
                             rdfs:range xsd:string ;
                             <http://ns.ontowiki.net/SysOnt/order> "20"^^xsd:string ;
                             rdfs:comment "Bsp: Meier, Meyer, Maier"@de ,
                                          "For Example Meier, Meyer, Maier"@en ;
                             rdfs:label "Alternative Namensschreibweisen"@de ,
                                        "Alternative writings of the surname"@en ;
                             owl:versionInfo "0.1 initial creation - 24.05.07"^^xsd:string ,
                                             "0.2 changed Domain from Professor to Person"^^xsd:string ,
                                             "2.0 under discussion"^^xsd:string .
				     
###  http://purl.org/pcp-on-web/ontology#firstName
:firstName rdf:type owl:DatatypeProperty ;
           rdfs:domain :Person ;
           rdfs:range rdfs:Literal;
           rdfs:comment "Rufname einer Person"@de ,
                        "The given first name of a person"@en ,
                        "given name of a person"@en ;
           rdfs:label "First Name"@en ,
                      "Rufname"@de ;
           owl:versionInfo "0.1 initial creation date: 18.05.2015"^^xsd:string ,
                           "0.1 initial creation date: 29.01.2015"^^xsd:string .


###  http://purl.org/pcp-on-web/ontology#forename
:forename rdf:type owl:DatatypeProperty ;
          rdfs:domain :Person ;
          rdfs:range rdfs:Literal;
          rdfs:comment "Forename(s) of a person, seperated by space"@en ,
                       "Vorname(n) einer Person, getrennt mit Leerzeichen"@de ;
          rdfs:label "Forename"@en ,
                     "Vorname"@de ;
          owl:versionInfo "0.1 initial creation date: 18.05.2015"^^xsd:string .

:surnameVariant rdf:type owl:DatatypeProperty ;
					rdfs:domain :Person ;
					rdfs:range rdfs:Literal ;
					rdfs:comment "Alternative spelling of a person's last name."@en ,
								"Alternative Schreibweise des Nachnamens einer Person."@de ;
					rdfs:label "Surname Variant"@en ,
							  "Name Variant"@de ;
					owl:versionInfo "PCP-on-Web 1 including property from merging Helmstedt data - 27.06.18"^^xsd:string .

:latinSurname rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Person ;
				   rdfs:range rdfs:Literal ;
                   rdfs:comment "Latinized last name of a person."@en ,
                                "Latinisierter Nachname einer Person."@de ;
                   rdfs:label "Latin Last Name"@en ,
                              "Latein Name"@de ;
                   owl:versionInfo "PCP-on-Web 1 including property from merging Helmstedt data - 27.06.18"^^xsd:string .
		
:StageOfLife rdf:type owl:Class ;
  rdfs:label "Lebensabschnitt"@de, "Stage of Life"@en ;
	dc:description "Dieses Konzept bezieht sich auf einen Lebensabschnitt."@de, "This concept refers to a stage of life."@en .

:Birth rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Geburt"@de, "Birth"@en ;
	dc:description "Gibt das Datum oder den Zeitraum der Geburt einer Person an."@de, "Gives the birthdate or the period of birth of a person."@en .
		
:Death rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Tod"@de, "Death"@en ;
	dc:description "Gibt das Datum oder den Zeitraum des Todes einer Person an."@de, "Gives the date or the period of death of a person."@en .
	
:hasStageOfLife rdf:type owl:DatatypeProperty ;
  rdfs:domain :Person ;
  rdfs:range :StageOfLife ;
  rdfs:label "hat Lebensabschnitt"@de, "has stage of life"@en ;
	dc:description "Beschreibt, dass eine Person einen Lebensabschnitt hat."@de, "Refers to the stage of life of a person."@en .
		
:Family rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Familie"@de, "family"@en ;
	dc:description "Dieses Konzept beschreibt die soziale Herkunft und Familienverhältnisse einer Person."@de, "This concept describes the social origin
		and the family relationship of a person."@en .
		
:spouse rdf:type owl:ObjectProperty ;
  rdfs:domain :Family ;
  rdfs:range :Person ;
  rdfs:label "Ehepartner"@de, "spouse"@en ;
	dc:description "Bezieht sich auf den Ehepartner (genersiches Maskulinum) einer Person."@de, "Refers to the spouse (generic masculine) of a person."@en .
		
:child rdf:type owl:ObjectProperty ;
  rdfs:domain :Family ;
  rdfs:range :Person ;
  rdfs:label "Kind"@de, "child"@en ;
	dc:description "Bezieht sich auf das Kind, Pflegekind, Adoptivkind einer Person."@de, "Refers to the cild, foster child, adoptive child of a person."@en .
	
:parent rdf:type owl:ObjectProperty ;
  rdfs:domain :Family ;
  rdfs:range :Person ;
  rdfs:label "Elternteil"@de, "parent"@en ;
	dc:description "Weist eine Person als Elternteil aus."@de, "Depicts a person as parent."@en .
		
:Location rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Ort"@de, "location"@en ;
	dc:description "Bezieht sich auf den geografischen Ort eines Lebensabschnitts einer Person sowie auf den Ort von Körperschaften,
		Medien (Verlagsort, Druckort), Ämtern, Landesherrschaft."@de, "Refers to the location of life stages of persons, corporate bodies, media
		(printing, publishing), offices, territories."@en .
		
:locationStageOfLife rdf:type owl:DatatypeProperty ;
  rdfs:domain :StageOfLife ;
  rdfs:range :Location ;
  rdfs:label "Ort eines Lebensabschnitts"@de, "location of a stage of life"@en ;
	dc:description "Bezieht sich auf den geografischen Ort eines Lebensabschnitts einer Person."@de,
		"Refers to the geographical location of a stage of life of a person."@en .
		
:locationCorporation rdf:type owl:DatatypeProperty ;
  rdfs:domain :Corporation ;
  rdfs:range :Location ;
  rdfs:label "Ort einer Körperschaft"@de, "location of a corporation"@en ;
	dc:description "Bezieht sich auf den geografischen Ort einer Körperschaft."@de,
		"Refers to the geographical location of a corporation/corporate body."@en .
				     
:locationEducation rdf:type owl:DatatypeProperty ;
  rdfs:domain :Education ;
  rdfs:range :Location ;
  rdfs:label "Ort der Ausbildung"@de, "location of education"@en ;
	dc:description "Bezieht sich auf den geografischen Ort der Ausbildung."@de,
		"Refers to the geographical location of education."@en .
		
:locationMembership rdf:type owl:DatatypeProperty ;
  rdfs:domain :Membership ;
  rdfs:range :Location ;
  rdfs:label "Ort der Mitgliedschaft"@de, "location of membership"@en ;
	dc:description "Bezieht sich auf den geografischen Ort einer Mitgliedschaft."@de,
		"Refers to the geographical location of a membership."@en .
				     
:locationOffice rdf:type owl:DatatypeProperty ;
  rdfs:domain :Office ;
  rdfs:range :Location ;
  rdfs:label "Ort eines Amts"@de, "location of an office"@en ;
	dc:description "Bezieht sich auf den geografischen Ort eines Amts."@de,
		"Refers to the geographical location of an office."@en .

:locationTerritory rdf:type owl:DatatypeProperty ;
  rdfs:domain :Territory ;
  rdfs:range :Location ;
  rdfs:label "Ort einer Landesherrschaft"@de, "location of a territory"@en ;
	dc:description "Bezieht sich auf den geografischen Ort einer Landesherrschaft."@de,
		"Refers to the geographical location of a territory."@en .	
	
:Denomination rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Konfession"@de, "Denomination"@en ;
	dc:description "Religionszugehörigkeit oder Glaubenbekenntnis einer Person."@de , "The religious denomination of a person."@en .
		
:hasDenomination rdf:type owl:ObjectProperty ;
  rdfs:domain :Person ;
  rdfs:range :Denomination ;
  rdfs:label "hat Konfession"@de, "has denomination"@en ;
	dc:description "Verweist auf das Glaubenbekenntnis einer Person und/oder deren Religionszugehörigkeit."@de,
		"Refers to the religious denomination of a person."@en .
	
:Education  rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Bildungsweg"@de, "Education"@en ;
	dc:description "Beschreibt den Bildungsweg einer Person."@de , "Decribes the education of a person."@en .
		
:Corporation rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Körperschaft"@de, "Corporation"@en ;
	dc:description "Bezeichnet eine Körperschaft."@de, "Refers to a corporation or corporate body."@en .
		
:PrimaryEducation  rdf:type owl:Class ;
  rdfs:subClassOf :Education ;
  rdfs:label "Primäre Bildung"@de, "primary education"@en ;
	dc:description "Bezieht sich auf die diversen vormodernen Schulformen und pädagogischen Bildungsangebote im Vorfeld eines Universitätsbesuchs
		im Territorium des Heiligen Römischen Reichs Deutscher Nation, z.B. Elementarschulen, Lateinschulen, Gymnasien, Klosterschulen,
		Pfarrschulen etc."@de , "Refers to the diverse early modern educational and pedagogical formas and institutions of primary education within the
		Holy Roman Empire."@en .		
		
:PrivateEducation rdf:type owl:Class ;
  rdfs:subClassOf :Education ;
  rdfs:label "Privatunterricht"@de, "private education"@en ;
	dc:description "Privatunterricht im Familienverband oder durch einen Privatlehrer."@de,
		"Private education by family members or a different private teacher."@en .
		
:UniversityStudies rdf:type owl:Class ;
  rdfs:subClassOf :Education ;
  rdfs:label "Studium"@de, "University studies"@en ;
	dc:description "Bezieht sich auf das Studium einer Person an einer vormodernen Universität."@de, "Refers to the university studies of a person at
		Early Modern Universities."@en .
				    
:Enrollment rdf:type owl:Class ;
  rdfs:subClassOf :UniversityStudies ;
  rdfs:label "Immatrikulation"@de, "Enrollment"@en ;
	dc:description "Bezieht sich auf die Einschreibung einer Person in das Universitätsverzeichnis, die (Universitäts-)Matrikel."@de, "Refers to the enrollment of a person
		into the universities' directory, the (university) 'Matrikel'."@en .

:hasEnrollment rdf:type owl:ObjectProperty ;
  rdfs:domain :Person ;
  rdfs:range :Enrollment ;
  rdfs:label "hat Matrikel"@de, "has enrollment"@en ;
	dc:description "Verweist auf eine Immatrikulation einer Person an einer Universität."@de, "Refers to the enrollment of a person at a university."@en .
				     
:enrollmentSemester rdf:type owl:DatatypeProperty ;
  rdfs:domain :Enrollment ;
  rdfs:range :Person ;
  rdfs:label "Semester der Immatrikulation"@de, "Semester of enrollment"@en ;
	dc:description "Nennt das Semester der Immatrikulation einer Person."@de, "Gives information of the semester in which a person got enrolled."@en .
				     
:AcademicDegree rdf:type owl:Class ;
  rdfs:subClassOf :StageOfLife ;
  rdfs:label "Akademische Grade"@de, "Academic Degree"@en ;
	dc:description "Bezeichnet akademische Abschlüsse, die an frühneuzeitlichen Universitäten in deutschen Territorien erworben werden konnten."@de , "..."@en .
				     
:Baccalaureus rdf:type owl:Class ;
  rdfs:subClassOf :AcademicDegree ;
  rdfs:label "Baccalaureus"@de, "Baccalaureus"@en ;
	dc:description "Bezeichnet den akademischen Grad eines Baccalaren/Baccalaureus. Kürzel: Bacc."@de , "Describes the academic degree of a bachelor/baccalaureus. 
			Abbreviation: Bacc."@en .
				     
:Licentiatus rdf:type owl:Class ;
  rdfs:subClassOf :AcademicDegree ;
  rdfs:label "Lizenziat"@de, "Licentiate"@en ;
	dc:description "Bezeichnet den akademischen Grad eines Lizenziaten, eines Baccalaren mit Lehrbefugnis. Zugleich einen Doktor der evang. Theologie
			(bis 1947 an deutschen Hochschulen). Kürzel: lic."@de , "Describes the academic degree of a Licentiate, which means a bachelor/baccalaureus
			with the right to teach. Also the academic title of a doctor of Evangelic theology (valid until 1947 in Germany). Abbreviation: lic."@en .
				     
:Magister rdf:type owl:Class ;
  rdfs:subClassOf :AcademicDegree ;
  rdfs:label "Magister"@de, "Magister"@en ;
	dc:description "Bezeichnet den akademischen Grad eines Magister. Kürzel: Mag."@de , "Describes the academic degree of a magister. Abbreviation: Mag."@en .
				     
:Doctor rdf:type owl:Class ;
  rdfs:subClassOf :AcademicDegree ;
  rdfs:label "Doctor"@de, "Doctor"@en ;
	dc:description "Bezeichnet den akademischen Grad eines Doktoren. Kürzel: Dr."@de , "Describes the academic degree of a doctor. Abbreviation: Dr."@en .
				     
:academicDegreeDate rdf:type owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:domain :AcademicDegree ;
  rdfs:range rdfs:Literal ;
  rdfs:label "Date of Academic Degree"@en, "Datum des Akademischen Grads"@de ;
  	dc:descritption "Gibt den Zeitpunkt an wann ein akademischer Grad erworben wurde."@de, "Gives the date of earning a degree from a valid datatype
		 xsd:date ,
                 xsd:gDay ,
                 xsd:gMonth ,
                 xsd:gMonthDay ,
                 xsd:gYear ,
                 xsd:gYearMonth"@en .
				     
:hasAcademicDegree rdf:type owl:ObjectProperty ;
  rdfs:domain :Person ;
  rdfs:range :AcademicDegree ;
  rdfs:label "hat akademischen Grad"@de, "has academic degree"@en ;
	dc:description "Gibt an, dass eine Person über einen akademischen Grad verfügt."@de, "Gives the academic degree of a person."@en .
		
:isStudentAt rdf:type owl:ObjectProperty ;
  rdfs:domain :Student ;
  rdfs:range :University ;
  rdfs:label "ist ein Student an"@de, "is a student at"@en ;
	dc:description "Bezieht sich auf die Information an welcher Korporation ein Student studiert."@de,
		"Refers to the information at which corporation a student studies."@en .
	
:University rdf:type owl:Class ;
  rdfs:subClassOf :Corporation ;
  rdfs:label "Universität"@de, "University"@en ;
	dc:description "Bezeichnet eine Lehr-, Bildungs- und Forschungskorporation die dazu priviligiert ist akademische Grade zu verleihen."@de ,
		"Refers to education and research corporations which own the privilige to award academic degrees."@en .
	
:Faculty rdf:type owl:Class ;
  rdfs:subClassOf :University ;
  rdfs:label "Fakultät"@de, "Faculty"@en ;
	dc:description "Bezieht sich auf eine körperschaftlich organisierte und auf ein Fachgebiet begrenzte Lehr- und Verwaltungseinheit einer Volluniversität,
		im frühneuzeitlichen Kontext: Philosophie, Theologie, Jurisprudenz, Medizin."@de, "Refers to corporated university divisions with specific subject
		areas, which are in an Early Modern context: Philosophy, Theology, Jurisprudence, Medicine."@en .
	
:FacultyOfPhilosophy rdf:type owl:Class ;
  rdfs:subClassOf :Faculty ;
  rdfs:label "Philosophische Fakultät"@de, "Faculty of Philosophy"@en ;
	dc:description "Bezeichnet die Philosophische Fakultät an einer Universität."@de, "Refers to the Faculty of Philosophy at Universities."@en .
		
:FacultyOfTheology rdf:type owl:Class ;
  rdfs:subClassOf :Faculty ;
  rdfs:label "Theologische Fakultät"@de, "Faculty of Theology"@en ;
	dc:description "Bezeichnet die Theologische Fakultät an einer Universität."@de, "Refers to the Faculty of Theology at Universities."@en .
		
:FacultyOfLaw rdf:type owl:Class ;
  rdfs:subClassOf :Faculty ;
  rdfs:label "Juristische Fakultät"@de, "Faculty of Law"@en ;
	dc:description "Bezeichnet die Juristische Fakultät an einer Universität."@de, "Refers to the Faculty of Law at Universities."@en .

:FacultyOfMedicine rdf:type owl:Class ;
