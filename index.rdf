:AcademicDegree rdf:type owl:Class ;
  rdfs:subClassOf :Graduation ;
  rdfs:label "Akademische Grade"@de, "Academic Degree"@en ;
	dc:description "Bezeichnet akademische Abschlüsse."@de , "Refers to Academic degrees."@en .

:AcademicOffice rdf:type owl:Class ;
  rdfs:subClassOf :Office ;
  rdfs:label "Akademisches Amt"@de, "Academic office"@en ;
