#################################################################
#    Object Properties
#################################################################

###  http://pcp-on-web.de/ontology#corporationDocument
:corporationDocument rdf:type owl:ObjectProperty ;
                     rdfs:domain :Corporation ;
                     rdfs:range :Document ;
                     rdfs:comment "Documents related to corporations and institutions, e.g. deeds of appointment ratified by the sovereignty."@en ,
                                  "Dokumente die mit Körperschaften und Institutionen in Verbindung stehen, z.B. Ernennungsurkunden ratifziert durch die Landesherrschaft."@de ;
                     rdfs:label "has Document"@en ,
                                "hat Dokument"@de .


###  http://pcp-on-web.de/ontology#corresponds
:corresponds rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/knows> ;
             rdfs:domain :Person ;
             rdfs:range :Person ;
             rdfs:comment "Assigns to the correspondence between persons, e.g. proven via editions or heritage plattforms such as KALLIOPE."@en ,
                          "Verweist auf die aus einem Dokument hervorgehende Korrespondenz zwischen Personen, z.B. nachgewiesen über Editionen oder Nachlassplattformen wie KALLIOPE."@de ;
             rdfs:label "corresponds"@en ,
                        "korrespondiert"@de .


###  http://pcp-on-web.de/ontology#derivedFrom
:derivedFrom rdf:type owl:ObjectProperty ;
             rdfs:domain owl:Thing ;
             rdfs:range owl:Thing ;
             rdfs:comment "Wird für das Alignment genutzt."@de ,
                          "Wird für das Alignment genutzt."@en ;
             rdfs:label "has been derived from"@en ,
                        "ist abgeleitet von"@de .


###  http://pcp-on-web.de/ontology#disputationHasDissertation
:disputationHasDissertation rdf:type owl:ObjectProperty ;
                            rdfs:domain :Disputation ;
                            rdfs:range :Dissertation ;
                            rdfs:comment "Assigns a disputation to a dissertation."@en ,
                                         "Weist einer Disputation eine Dissertation zu."@de ;
                            rdfs:label "Disputation hat Dissertation"@de ,
                                       "disputation has dissertation"@en .


###  http://pcp-on-web.de/ontology#enrollment
:enrollment rdf:type owl:ObjectProperty ;
            rdfs:domain :Enrollment ;
            rdfs:range :Student ;
            rdfs:comment "Im Matrikelbuch eingeschriebener Student."@de ,
                         "Most likely latinized name of a student at the enrollment document of a university."@en ;
            rdfs:label "eingeschriebener Student"@de ,
                       "enrolled student"@en .


###  http://pcp-on-web.de/ontology#female
:female rdf:type owl:ObjectProperty ;
        rdfs:domain :Person ;
        rdfs:range :Gender ;
        rdfs:comment "Refers to the female sex or female gender of a person."@en ,
                     "Verweist auf das biologische weibliche Geschlecht oder die weibliche Geschlechtsidentität einer Person."@de ;
        rdfs:label "female"@en ,
                   "weiblich"@de .


###  http://pcp-on-web.de/ontology#genderqueer
:genderqueer rdf:type owl:ObjectProperty ;
             rdfs:domain :Person ;
             rdfs:range :Gender ;
             rdfs:comment "Refers to the genderqueer identity of a person."@en ,
                          "Verweist auf die nicht-binäre Geschlechtsidentität einer Person."@de ;
             rdfs:label "divers"@de ,
                        "genderqueer"@en .


###  http://pcp-on-web.de/ontology#geonameLocation
:geonameLocation rdf:type owl:ObjectProperty ;
                 rdfs:domain :Location ;
                 rdfs:range rdfs:Resource ;
                 rdfs:comment "Die URL des Ressourcenstandorts in http://geonames.org."@de ,
                              "The Resource location's URL in http://geonames.org."@en ;
                 rdfs:label "Geoname Ort"@de ,
                            "Geoname location"@en ;
                 owl:versionInfo "PCP-on-Web 1 including property from merging Helmstedt data - 27.06.18" .


###  http://pcp-on-web.de/ontology#hasAuctionCatalogue
:hasAuctionCatalogue rdf:type owl:ObjectProperty ;
                     rdfs:domain :PrivateLibrary ;
                     rdfs:range :AuctionCatalogue ;
                     <http://purl.org/dc/terms/description> "Adds an auction catalogue to a private library."@en ,
                                                            "Verknüpft eine Privatbibliothek mit einem Auktionskatalog."@de ;
                     rdfs:label "has auction catalogue"@en ,
                                "hat Auktionskatalog"@de .


###  http://pcp-on-web.de/ontology#hasAuthor
:hasAuthor rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://purl.org/dc/terms/creator> ;
           rdfs:domain :Document ;
           rdfs:range :Corporation ,
                      :Person ;
           rdfs:comment "Refers to the authorship of a document."@en ,
                        "Verweist auf die Autorenschaft eines Dokuments."@de ;
           rdfs:label "hat Autor"@de ,
                      "hat author"@en .


###  http://pcp-on-web.de/ontology#hasChild
:hasChild rdf:type owl:ObjectProperty ;
