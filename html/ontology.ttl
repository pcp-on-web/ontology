                rdfs:comment "Refers to the contributor in the developement process of a document."@en ,
                             "Verweist auf eine Entität, die am Entstehungsprozess eines Dokuments beteiligt ist."@de ;
                rdfs:label "has contributor"@en ,
                           "hat Beteiligten"@de .


