         rdfs:subClassOf :Person ;
         rdfs:comment "Bezeichnet einen an der Universität immatrikulierten Studenten."@de ,
                      "Refers to an enrolled student at a University."@en ;
         rdfs:label "Student"@de ,
                    "Student"@en .


###  http://pcp-on-web.de/ontology#Subject
:Subject rdf:type owl:Class .


###  http://pcp-on-web.de/ontology#SummerSemester
:SummerSemester rdf:type owl:Class ;
                rdfs:subClassOf :Semester ;
                rdfs:comment "Das halbjährige Sommersemester dauert von April bis September."@de ,
                             "The biannual summer semester starts in April and ends in September."@en ;
                rdfs:label "Sommersemester"@de ,
                           "Summer Semester"@en .


###  http://pcp-on-web.de/ontology#TeachingPractices
:TeachingPractices rdf:type owl:Class ;
                   rdfs:subClassOf :StageOfLife ;
